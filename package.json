{
	"name": "kilo-scheduler",
	"displayName": "Kilo Scheduler",
	"description": "A task scheduler for Kilo Code",
	"publisher": "unbrained",
	"version": "0.0.14",
	"icon": "assets/icons/scheduler-icon-light.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0",
		"node": "20.18.1"
	},
	"author": {
		"name": "Stefan Preu",
		"url": "https://github.com/unbraind"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/unbrained/IDE-scheduler"
	},
	"homepage": "https://github.com/unbrained/IDE-scheduler",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama",
		"scheduler",
		"kilo",
		"kilo-code",
		"kilo-scheduler"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"extensionDependencies": [
		"kilocode.kilo-code"
	],
	"contributes": {
		"configuration": {
			"title": "Kilo Scheduler",
			"properties": {
				"kilo-scheduler.experimental.activityBadge": {
					"type": "boolean",
					"default": false,
					"description": "Experimental: show the number of active schedules as a badge on the Kilo Scheduler Activity Bar icon."
				,
				"kilo-scheduler.experimental.crossIde": {
					"type": "boolean",
					"default": false,
					"description": "Experimental (WIP): Enable cross-IDE scheduling interface + Agent-to-Agent trigger support. Very early WIP (cline, roo-code, Continue, Cursor, Claude Code, Gemini CLI, Qwen Coder CLI)."
				},
				"kilo-scheduler.experimental.agents.kilocode.enabled": {
					"type": "boolean",
					"default": true,
					"description": "Enable Kilo Code adapter (experimental cross-IDE)."
				},
				"kilo-scheduler.experimental.agents.kilocode.allowedActions": {
					"type": "array",
					"description": "Allowed A2A actions for Kilo Code adapter.",
					"items": { "type": "string", "enum": ["trigger", "list", "setActive"] },
					"default": ["trigger", "list", "setActive"]
				},
				"kilo-scheduler.experimental.agents.cline.enabled": {
					"type": "boolean",
					"default": false,
					"description": "Enable Cline adapter (experimental cross-IDE)."
				},
				"kilo-scheduler.experimental.agents.cline.allowedActions": {
					"type": "array",
					"description": "Allowed A2A actions for Cline adapter.",
					"items": { "type": "string", "enum": ["trigger", "list", "setActive"] },
					"default": ["trigger"]
				},
				"kilo-scheduler.experimental.agents.rooCode.enabled": {
					"type": "boolean",
					"default": false,
					"description": "Enable Roo Code adapter (experimental cross-IDE)."
				},
				"kilo-scheduler.experimental.agents.rooCode.allowedActions": {
					"type": "array",
					"description": "Allowed A2A actions for Roo Code adapter.",
					"items": { "type": "string", "enum": ["trigger", "list", "setActive"] },
					"default": ["trigger"]
				},
				"kilo-scheduler.experimental.agents.continue.enabled": {
					"type": "boolean",
					"default": false,
					"description": "Enable Continue adapter (experimental cross-IDE)."
				},
				"kilo-scheduler.experimental.agents.continue.allowedActions": {
					"type": "array",
					"description": "Allowed A2A actions for Continue adapter.",
					"items": { "type": "string", "enum": ["trigger", "list", "setActive"] },
					"default": ["trigger"]
				},
				"kilo-scheduler.experimental.agents.cursor.enabled": {
					"type": "boolean",
					"default": false,
					"description": "Enable Cursor adapter (experimental cross-IDE)."
				},
				"kilo-scheduler.experimental.agents.cursor.allowedActions": {
					"type": "array",
					"description": "Allowed A2A actions for Cursor adapter.",
					"items": { "type": "string", "enum": ["trigger", "list", "setActive"] },
					"default": ["trigger"]
				},
				"kilo-scheduler.experimental.agents.claudeCode.enabled": {
					"type": "boolean",
					"default": false,
					"description": "Enable Claude Code adapter (experimental cross-IDE)."
				},
				"kilo-scheduler.experimental.agents.claudeCode.allowedActions": {
					"type": "array",
					"description": "Allowed A2A actions for Claude Code adapter.",
					"items": { "type": "string", "enum": ["trigger", "list", "setActive"] },
					"default": ["trigger"]
				},
				"kilo-scheduler.experimental.agents.geminiCli.enabled": {
					"type": "boolean",
					"default": false,
					"description": "Enable Google Gemini CLI adapter (experimental cross-IDE)."
				},
				"kilo-scheduler.experimental.agents.geminiCli.allowedActions": {
					"type": "array",
					"description": "Allowed A2A actions for Gemini CLI adapter.",
					"items": { "type": "string", "enum": ["trigger", "list", "setActive"] },
					"default": ["trigger"]
				},
				"kilo-scheduler.experimental.agents.qwenCli.enabled": {
					"type": "boolean",
					"default": false,
					"description": "Enable Qwen Coder CLI adapter (experimental cross-IDE)."
				},
				"kilo-scheduler.experimental.agents.qwenCli.allowedActions": {
					"type": "array",
					"description": "Allowed A2A actions for Qwen CLI adapter.",
					"items": { "type": "string", "enum": ["trigger", "list", "setActive"] },
					"default": ["trigger"]
				}
			}
		}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "kilo-scheduler-ActivityBar",
					"title": "Kilo Scheduler",
					"icon": {
						"light": "assets/icons/scheduler-icon-light.png",
						"dark": "assets/icons/scheduler-icon-dark.png"
					}
				}
			]
		},
		"views": {
			"kilo-scheduler-ActivityBar": [
				{
					"type": "webview",
					"id": "kilo-scheduler.SidebarProvider",
					"name": ""
				}
			]
		},
		"commands": [
			{
				"command": "kilo-scheduler.reloadWindowDev",
				"title": "Reload Window (Dev Only)",
				"icon": "$(refresh)",
				"category": "Developer"
			},
			{
				"command": "kilo-scheduler.openKiloCodeExtension",
				"title": "Open Kilo Code Extension",
				"icon": {
					"light": "assets/icons/scheduler-icon-light.png",
					"dark": "assets/icons/scheduler-icon-dark.png"
				}
			},
			{
				"command": "kilo-scheduler.a2a.trigger",
				"title": "A2A: Trigger Agent (Experimental)",
				"category": "Kilo Scheduler"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "kilo-scheduler.reloadWindowDev",
					"group": "navigation@0",
					"when": "view == kilo-scheduler.SidebarProvider && kiloSchedulerDevMode"
				},
				{
					"command": "kilo-scheduler.openKiloCodeExtension",
					"group": "navigation@1",
					"when": "view == kilo-scheduler.SidebarProvider"
				}
			]
		}
	},
	"scripts": {
		"build": "npm run vsix",
		"build:webview": "cd webview-ui && npm run build",
		"build:esbuild": "node esbuild.js --production",
		"compile": "tsc -p . --outDir out && node esbuild.js",
		"install:all": "npm install npm-run-all && npm-run-all -l -p install-*",
		"install-extension": "npm install",
		"install-webview": "cd webview-ui && npm install",
		"install-e2e": "cd e2e && npm install",
		"lint": "npm-run-all -l -p lint:*",
		"lint:extension": "eslint src --ext ts",
		"lint:webview": "cd webview-ui && npm run lint",
		"lint:e2e": "cd e2e && npm run lint",
		"check-types": "npm-run-all -l -p check-types:*",
		"check-types:extension": "tsc --noEmit",
		"check-types:webview": "cd webview-ui && npm run check-types",
		"check-types:e2e": "cd e2e && npm run check-types",
		"package": "npm-run-all -l -p build:webview build:esbuild check-types lint",
		"pretest": "npm run compile",
		"dev": "cd webview-ui && npm run dev",
		"test": "npm run test:extension && npm run test:webview",
		"test:extension": "jest",
		"test:webview": "cd webview-ui && npm run test",
		"test:e2e": "npm run vscode-test && cd e2e && npm run test",
		"test:kiloservice": "npm run vscode-test && cd e2e && npm run test:kiloservice",
		"test:e2e:all": "npm run vscode-test && cd e2e && npm run test:all",
		"prepare": "husky",
		"publish:marketplace": "vsce publish && ovsx publish",
		"publish": "npm run build && changeset publish && npm install --package-lock-only",
		"version-packages": "changeset version && npm install --package-lock-only",
		"vscode:prepublish": "npm run build:webview && npm run build:esbuild",
		"vsix": "rimraf bin && mkdirp bin && npx vsce package --out bin",
		"watch": "npm-run-all -l -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"watch-tests": "tsc -p . -w --outDir out",
		"changeset": "changeset",
		"knip": "knip --include files",
		"clean": "npm-run-all -l -p clean:*",
		"clean:extension": "rimraf bin dist out",
		"clean:webview": "cd webview-ui && npm run clean",
		"clean:e2e": "cd e2e && npm run clean",
		"vscode-test": "npm-run-all -l -p vscode-test:*",
		"vscode-test:extension": "tsc -p . --outDir out && node esbuild.js",
		"vscode-test:webview": "cd webview-ui && npm run build",
		"update-contributors": "node scripts/update-contributors.js",
		"generate-types": "tsx scripts/generate-types.mts"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.12.5",
		"@anthropic-ai/sdk": "^0.37.0",
		"@anthropic-ai/vertex-sdk": "^0.7.0",
		"@aws-sdk/client-bedrock-runtime": "^3.817.0",
		"@genkit-ai/mcp": "^1.18.0",
		"@google-cloud/vertexai": "^1.9.3",
		"@google/generative-ai": "^0.18.0",
		"@mistralai/mistralai": "^1.3.6",
		"@modelcontextprotocol/sdk": "^1.7.0",
		"@types/clone-deep": "^4.0.4",
		"@types/pdf-parse": "^1.1.4",
		"@types/tmp": "^0.2.6",
		"@types/turndown": "^5.0.5",
		"@types/vscode": "^1.95.0",
		"@vscode/codicons": "^0.0.36",
		"axios": "^1.7.4",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^7.0.0",
		"diff-match-patch": "^1.0.5",
		"fast-deep-equal": "^3.1.3",
		"fast-xml-parser": "^4.5.1",
		"fastest-levenshtein": "^1.0.16",
		"fzf": "^0.5.2",
		"get-folder-size": "^5.0.0",
		"globby": "^14.0.2",
		"i18next": "^24.2.2",
		"isbinaryfile": "^5.0.2",
		"js-tiktoken": "^1.0.19",
		"mammoth": "^1.8.0",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
		"node-ipc": "^12.0.0",
		"openai": "^4.78.1",
		"os-name": "^6.0.0",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"pkce-challenge": "^4.1.0",
		"posthog-node": "^4.7.0",
		"pretty-bytes": "^6.1.1",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.4.0",
		"reconnecting-eventsource": "^1.6.4",
		"say": "^0.16.0",
		"serialize-error": "^12.0.0",
		"simple-git": "^3.27.0",
		"sound-play": "^1.1.0",
		"string-similarity": "^4.0.4",
		"strip-ansi": "^7.1.0",
		"strip-bom": "^5.0.0",
		"tmp": "^0.2.3",
		"turndown": "^7.2.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.10",
		"@changesets/types": "^6.0.0",
		"@dotenvx/dotenvx": "^1.41.0",
		"@types/debug": "^4.1.12",
		"@types/diff": "^7.0.2",
		"@types/diff-match-patch": "^1.0.36",
		"@types/glob": "^8.1.0",
		"@types/jest": "^29.5.14",
		"@types/node": "20.x",
		"@types/node-ipc": "^9.2.3",
		"@types/string-similarity": "^4.0.2",
		"@typescript-eslint/eslint-plugin": "^8.30.1",
		"@typescript-eslint/parser": "^8.32.1",
		"@vscode/vsce": "^3.3.2",
		"esbuild": "^0.25.2",
		"eslint": "^9.25.1",
		"execa": "^9.5.2",
		"glob": "^11.0.1",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-simple-dot-reporter": "^1.0.5",
		"knip": "^5.44.4",
		"lint-staged": "^15.2.11",
		"mkdirp": "^3.0.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.2.5",
		"tsup": "^8.4.0",
		"tsx": "^4.19.3",
		"typescript": "^5.4.5",
		"zod-to-ts": "^1.2.0"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,md}": [
			"prettier --write"
		],
		"src/**/*.{ts,tsx}": [
			"npx eslint -c .eslintrc.json --max-warnings=0 --fix"
		],
		"webview-ui/**/*.{ts,tsx}": [
			"npx eslint -c webview-ui/.eslintrc.json --max-warnings=0 --fix"
		]
	}
}
