{
  "files.exclude": {
    "out": false,
    "dist": false
  },
  "search.exclude": {
    "out": true,
    "dist": true
  },
  "typescript.tsc.autoDetect": "off",
  "codium.codeCompletion.enable": false,
  "claudeCodeChat.permissions.yoloMode": true,
  "chatgpt.openOnStartup": true,
  "agent-scheduler.experimental.crossIde": true,
  "agent-scheduler.experimental.autoMapOnStartup": true,
  "agent-scheduler.experimental.http.enabled": true,
  "agent-scheduler.experimental.mcp.enabled": true,
  "agent-scheduler.experimental.mcp.http.enabled": true,
  "agent-scheduler.experimental.grpc.enabled": false,
  "agent-scheduler.experimental.auth.http.required": false,
  "agent-scheduler.experimental.auth.mcp.required": false,
  "agent-scheduler.experimental.auth.grpc.required": false
}
